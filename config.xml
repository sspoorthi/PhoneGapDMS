<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>dms</name>
    <description>
        Disaster Management System- An App which guides app users in case of disasters.
    </description>
    <author email="support.dms@philips.com" href="">
        Philips DMS Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.3" />
    <engine name="android" spec="~6.2.3" />
    <icon src="launcher.png" />
    <plugin name="call-number" spec="https://github.com/Rohfosho/CordovaCallNumberPlugin.git" />
</widget>
